bTab:CreateConfigSystem("left") --this is the config system

       task.spawn(function ()
            while true do
                wait()
                      if getgenv().Highlight == true then
                      LocalHL.Parent = game.Players.LocalPlayer.Character
                       LocalHL.FillColor = Color3.fromRGB(0, 0, 0)
                       LocalHL.OutlineColor = Color3.fromRGB(0, 254, 252)
                   else
                       LocalHL.Parent = game.CoreGui
                   end
            end
        end)
       
       task.spawn(function ()
            while true do
                wait()
                if getgenv().CuhChams then
                    for i,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                        if v:IsA("BasePart") then
                            v.Material = "ForceField"
                        end
                    end
                else
                    for i,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                        if v:IsA("BasePart") then
                            v.Material = "Plastic"
                        end
                    end
                end
            end
        end)

getgenv().Spin = false
getgenv().SpinPower = 5
getgenv().CursorSpin = false
getgenv().CursorSpeed = 5
getgenv().Rainbow = false
getgenv().HitSounds = false
getgenv().HitSoundsID = "rbxassetid://1053296915"
getgenv().Volume = 10


game:GetService("RunService").Heartbeat:connect(function()

if getgenv().HitSounds then 
    for i,v in pairs(LocalPlayer.Character:GetDescendants()) do 
        if v:IsA("Sound") and v.Name == "ShootSound" then 
            v.SoundId = getgenv().HitSoundsID
            v.Volume  = getgenv().Volume
        end 
    end 
end

if Swastika.Visible then 
    CursorPath.Swastika.Position = UDim2.fromOffset(LocalMouse.X - 43, LocalMouse.Y - 39)

    if getgenv().Rainbow then 
        CursorPath.Swastika.TextColor3 = Color3.fromHSV(tick() % 6 / 6, 1, 1)
    end 

    if getgenv().CursorSpin == true then 
        CursorPath.Swastika.Rotation = CursorPath.Swastika.Rotation + getgenv().CursorSpeed
    end 
end 
 
if getgenv().Spin then
game:GetService("Players").LocalPlayer.PlayerGui.MainScreenGui.Aim.Rotation = game:GetService("Players").LocalPlayer.PlayerGui.MainScreenGui.Aim.Rotation + getgenv().SpinPower
end
end)
