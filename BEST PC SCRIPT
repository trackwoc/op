local testSection = VisTab:CreateSector("Custom Character", "right")  --you can  change the section code, for example "testsection" can be changed to "FunnyCoolSection" etc.

function Weld(x,y)
       local W = Instance.new("Weld")
       W.Part0 = x
       W.Part1 = y
       local CJ = CFrame.new(x.Position)
       local C0 = x.CFrame:inverse()*CJ
       local C1 = y.CFrame:inverse()*CJ
       W.C0 = C0
       W.C1 = C1
       W.Parent = x
end

